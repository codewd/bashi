<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mewin.entity">
	<class name="Order" table="t_order">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="name" length="15" />
		<property name="idCard" length="50" />
		<property name="phone" length="15" />
		<property name="jg" length="15" />
		<property name="address" length="50" />
		<property name="yuanwang" length="500" />
		<property name="cr" />
		<property name="lr" />
		<property name="et" />
		<property name="ht" length="50" />
		<property name="gotime" type="java.util.Date" />
		<property name="backtime" type="java.util.Date" />
		<property name="time" type="timestamp" />
	</class>
</hibernate-mapping>