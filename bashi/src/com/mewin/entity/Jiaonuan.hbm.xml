<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mewin.entity">
	<class name="Jiaonuan" table="t_jiaonuan">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="name" length="50" />
		<property name="idCard" length="50" />
		<property name="phone" length="50" />
		<property name="address" length="50" />
		<property name="yuanwang" length="2000" />
		<property name="time" type="java.util.Date" />
	</class>
</hibernate-mapping>